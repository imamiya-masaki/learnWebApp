{
  "extends": ["next/core-web-vitals", "plugin:@typescript-eslint/recommended"],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".jsx", ".tsx", ".ts"] }],
    "ts(2691)": "off"
  }
}
